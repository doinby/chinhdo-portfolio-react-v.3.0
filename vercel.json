{
   "version": 2,
   "builds": [
      {
         "src": "./server/main.ts",
         "use": "@vercel/node"
      },
      {
         "src": "./client/build",
         "use": "@vercel/static"
      }
   ],
   "rewrites": [
      {
         "source": "/api/v1/(.*)",
         "destination": "/api/v1"
      }
   ]
}